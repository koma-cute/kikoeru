{
  "name": "kikoeru",
  "version": "0.3.1",
  "description": "Web media player, specifically for voice works.",
  "main": "src/server/index.jsx",
  "bin": "src/server/index.js",
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "dist/**/*",
      "static/**/*"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "start": "cross-env NODE_ENV=production node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "profile": "webpack --mode production --profile --json > webpack-stats.json",
    "scan": "node ./src/server/filesystem/scanner.js"
  },
  "author": "nortonandrews",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^5.2.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "htmlparser2": "^3.10.1",
    "knex": "^0.19.5",
    "limit-promise": "^1.0.4",
    "memorystore": "^1.6.2",
    "natural-orderby": "^2.0.3",
    "node-fetch": "^2.6.0",
    "recursive-readdir": "^2.2.2",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-inferno": "^6.0.5",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-inferno-app": "^7.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^4.3.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "inferno": "^7.4.2",
    "inferno-component": "^7.4.2",
    "inferno-create-element": "^7.4.2",
    "inferno-redux": "^7.4.2",
